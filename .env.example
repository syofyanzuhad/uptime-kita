VITE_APP_NAME="${APP_NAME}"

# admin credential
ADMIN_EMAIL=admin@syofyanzuhad.dev
ADMIN_PASSWORD=password123

# google oauth https://developers.google.com/identity/protocols/oauth2?hl=id
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
# GOOGLE_REDIRECT_URI=

# telegram bot token https://t.me/botfather
TELEGRAM_BOT_TOKEN=

# email configuration with https://resend.com/
RESEND_API_KEY=

# Uptime Kita Deployment Configuration
# Options: 'saas' or 'self-hosted'
UPTIME_DEPLOYMENT_TYPE=self-hosted

# SaaS Configuration (only applies when UPTIME_DEPLOYMENT_TYPE=saas)
UPTIME_SAAS_EMAIL_DAILY_LIMIT=10
UPTIME_SAAS_TELEGRAM_DAILY_LIMIT=50
UPTIME_SAAS_MAX_MONITORS=5
UPTIME_SAAS_MIN_CHECK_FREQUENCY=5
UPTIME_SAAS_DATA_RETENTION=30
UPTIME_SAAS_CUSTOM_STATUS_PAGES=false
UPTIME_SAAS_TEAM_COLLABORATION=false
UPTIME_SAAS_API_ACCESS=false
UPTIME_SAAS_EXPORT_DATA=false
UPTIME_SAAS_WHITE_LABEL=false

# Self-Hosted Configuration (only applies when UPTIME_DEPLOYMENT_TYPE=self-hosted)
# Set to 0 for unlimited
UPTIME_SELF_EMAIL_DAILY_LIMIT=0
UPTIME_SELF_TELEGRAM_DAILY_LIMIT=0
UPTIME_SELF_MAX_MONITORS=0
UPTIME_SELF_MIN_CHECK_FREQUENCY=1
UPTIME_SELF_DATA_RETENTION=0
UPTIME_SELF_CUSTOM_STATUS_PAGES=true
UPTIME_SELF_TEAM_COLLABORATION=true
UPTIME_SELF_API_ACCESS=true
UPTIME_SELF_EXPORT_DATA=true
UPTIME_SELF_WHITE_LABEL=true